{
    "alias": "eventum",
    "chmod": "0755",
    "finder": [
        {
            "name": [
                "eventum.php"
            ],
            "in": "."
        },
        {
            "name": [
                "*.php"
            ],
            "in": "src"
        },
        {
            "name": [
                "*.php"
            ],
            "in": "compat"
        },
        {
            "name": [
                "*.php"
            ],
            "exclude": [
                "tests", "Tests", "Tester", "*Tester.php"
            ],
            "in": "vendor"
        },
        {
            "name": [
                "schema.json"
            ],
            "in": "vendor/herrera-io/phar-update/res"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "main": "eventum.php",
    "git-version": "package_version",
    "output": "eventum.phar",
    "stub": true
}
